
<% @tours.each do |tour| %>
	<div class="booyah-box col-xs-10 col-xs-offset-1">
		<h1> <%= tour.name %></h1> 
		<p> <%= tour.description %> </p> <br />
		<p> <%= tour.state %> </p> <br />
		<p> <%= tour.country %> </p> <br />
		<p> <%= tour.difficulty %> </p> <br />
		<p> <%= tour.surface %> </p> <br />
		<p> <%= tour.distance %> </p> <br />
	</div>

	
<% end %>



	